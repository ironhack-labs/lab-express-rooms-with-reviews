{{#if username}}
        <h1> Welcome {{username}}</h1>
{{else}}
        <p>This is your profile page my friend!</p>
{{/if}}
        <button><a href="/room/create">New room</a></button>
<p>The room you currently have are:</p>

{{#each foundRooms}}
<div class="flex-container wrap">
        <ul>
        <li value="{{_id}}">{{name}}</li>
        <li>{{description}}</li>
        <img src="{{image}}">

        <li>By {{username}}</li>
        <div>
        <a class="edit" href="/room/{{id}}/edit">Edit</a>
        <a class="edit" href="/room/{{id}}/delete">Delete</a>
        <a class="edit" href="/room/{{id}}/review">Reviews</a>
        </div>
        </ul>
        <br>
</div>
{{/each }}

